<div class="container">


<h2>Titlle</h2>
<div class="mktitle">
<%= @wiki.title %>
</div>
<h2>Body</h2>
<div class="mkbody">
<%= markdown(@wiki.body) %>
</div>

<% if policy(@wiki).update? %>
<%= link_to 'Edit', edit_wiki_path(@wiki) %>
<%= link_to_if current_user == @wiki.user, "Collaborators", wiki_collaborators_path(@wiki) %>
<% end %>

<%= link_to 'Back', wikis_path %>

</div>
